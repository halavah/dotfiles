{
  "defaultFontFamily": {
  "standard": null,
  "serif": null,
  "sansSerif": null,
  "monospace": null
  },
  "autoHideMenuBar": false,
  "searchService": [
    ["Search with Google", "https://google.com/search?q=%s"]
  ],
  /*
    „ÄÅ
  */
  "keyBinding": {
    // File
    "New": "Ctrl+N",

    "Open Quickly...": "Ctrl+P",

    "Reopen Closed File": "Ctrl+Shift+T",
    "Clear Items": "Ctrl+Shift+Alt+T",

    "Open File Location...": "Alt+I",
    "Open...": "Alt+O",
    "Open Folder...": "Alt+P",

    "PDF": "Ctrl+F1",
    "HTML": "Ctrl+F2",

    "Save": "Ctrl+S",
    "Save As...": "Ctrl+Shift+S",
    "Preferences...": "Ctrl+Shift+P",

    // Edit
    "Find...": "Ctrl+F",
    "Find Next": "F2",
    "Find Previous": "Shift+F2",

    "Replace": "Ctrl+R",

    "Undo": "Ctrl+Z",
    "Redo": "Ctrl+Shift+Z",

    "Cut": "Ctrl + X",

    "Select All": "Ctrl+A",
    "Select Word": "Ctrl+W",

    // Paragraph
    "Heading 1": "Ctrl+Alt+1",
    "Heading 2": "Ctrl+Alt+2",
    "Heading 3": "Ctrl+Alt+3",
    "Heading 4": "Ctrl+Alt+4",
    "Heading 5": "Ctrl+Alt+5",
    "Heading 6": "Ctrl+Alt+6",
    "Paragraph": "Ctrl+Shift+N",

    "Code Fences": "Ctrl+Shift+K",

    // Format
    "Insert Table": "Ctrl+T",
    "Strike": "Ctrl+Y",
    "Underline": "Ctrl+U",

    "Strong": "Ctrl+B",

    "Comment": "Ctrl+/",

    // View
    "Toggle Sidebar": "Alt+1",

    "Zoom In": "Ctrl+=",
    "Zoom Out": "Ctrl+-",

    "Toggle Fullscreen": "F11",
    "Toggle DevTools": "F12",
    "Source Code mode": "Ctrl+F12"
  },
  "monocolorEmoji": false,
  "autoSaveTimer" : 3,
  "maxFetchCountOnFileList": 500,
  "flags": []
}