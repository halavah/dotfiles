  // Yarn：终端窗口输出
  "yarn.runInTerminal": true,
  // Yarn：保持终端输出
  "yarn.dontHideOutputOnSuccess": false,

  // MySQL：查询每页数量
  "database-client.defaultSelectLimit": 30,
  // MySQL：配置格式方言
  "sql-formatter.dialect": "sql",
  // MySQL：关键字转大写
  "sql-formatter.uppercase": false,
  // MySQL：查询间换行数
  "sql-formatter.linesBetweenQueries": 2,
  // MySQL：默认格式程序
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },

  // Emmet：缩进触发扩展
  "emmet.triggerExpansionOnTab": true,
  // Emmet：显式语法提示
  "emmet.showSuggestionsAsSnippets": true,
  // Emmet：显示缩写建议
  "emmet.showAbbreviationSuggestions": true,
  // Emmet：显示标记语言
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  // Emmet：识别语法场景
  "emmet.includeLanguages": {
    "vue": "html",
    "wpy": "html",
    "vue-html": "html",
    "javascript": "javascriptreact"
  },

  // Eslint：开启保存触发
  "eslint.run": "onSave",
  // Eslint：开启自动格式
  "eslint.format.enable": true,
  // Eslint：开启状态显示
  "eslint.alwaysShowStatus": true,
  // Eslint：关闭代码检查
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  // Eslint：关闭代码检查
  "less.lint.zeroUnits": "error",
  "css.lint.duplicateProperties": "error",
  "scss.lint.duplicateProperties": "error",
  // Eslint：关闭代码检查
  "html.validate.scripts": false,
  "html.format.indentInnerHtml": true,
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  // Eslint：语法检查场景
  "eslint.options": { "extensions": [".js", ".vue"] },
  // Eslint：开启代码检查
  "eslint.validate": [
    "vue",
    "html",
    "vue-html",
    "javascript",
    "javascriptreact"
  ],

  // Prettier：换行的字符数
  "prettier.printWidth": 80,
  // Prettier：缩进字符数量
  "prettier.tabWidth": 2,
  // Prettier：使用空格缩进
  "prettier.useTabs": false,
  // Prettier：句尾添加分号
  "prettier.semi": false,
  // Prettier：单引代替双引
  "prettier.singleQuote": true,
  // Prettier：引用的Props
  "prettier.quoteProps": "as-needed",
  // Prettier：单引代替双引
  "prettier.jsxSingleQuote": false,
  // Prettier：结束行无逗号
  "prettier.trailingComma": "all",
  // Prettier：数组文字括号
  "prettier.bracketSpacing": true,
  // Prettier：拒绝单独一行
  "prettier.jsxBracketSameLine": false,
  // Prettier：箭函省略括号
  "prettier.arrowParens": "always",
  // Prettier：格式全部内容
  "prettier.rangeStart": 0,
  "prettier.rangeEnd": "Infinity",
  // Prettier：无需开头声明
  "prettier.requirePragma": false,
  // Prettier：无需自动插入
  "prettier.insertPragma": false,
  // Prettier：忽略敏感格式
  "prettier.proseWrap": "preserve",
  // Prettier：忽略空白空间
  "prettier.htmlWhitespaceSensitivity": "css",
  // Prettier：VUE脚本样式
  "prettier.vueIndentScriptAndStyle": false,
  // Prettier：结尾自动换行
  "prettier.endOfLine": "lf",
  // Prettier：嵌入格式引号
  "prettier.embeddedLanguageFormatting": "auto",
  // Prettier：是否需要文件（.prettierignore）
  "prettier.requireConfig": false,
  // Prettier：配置忽略文件（.prettierignore）
  "prettier.ignorePath": ".prettierignore",

  // Prettier：配置文件格式（CSS）
  "[css]": {
    // 编辑：基于缩进折叠
