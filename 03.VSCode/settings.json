      // Prettier：忽略空白空间
      "htmlWhitespaceSensitivity": "css",
      // Prettier：VUE脚本样式
      "vueIndentScriptAndStyle": false,
      // Prettier：结尾自动换行
      "endOfLine": "lf",
      // Prettier：嵌入格式引号
      "embeddedLanguageFormatting": "auto",
      // Prettier：是否需要文件（.prettierignore）
