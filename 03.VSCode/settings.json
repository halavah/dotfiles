  // oneDarkPro：解决斜体问题
  "oneDarkPro.italic": false,
  // oneDarkPro：部分字体粗体
  "oneDarkPro.bold": true,
  // oneDarkPro：MD格式的渲染
  "oneDarkPro.markdownStyle": true,
  // oneDarkPro：设置编辑主题
  "oneDarkPro.editorTheme": "vscode",

  // PartialDiff：禁止反馈数据
  "partialDiff.enableTelemetry": false,
  // PartialDiff：关闭右键菜单
  "partialDiff.hideCommandsOnContextMenu": true,

  // Npm：终端窗口输出
  "npm.runInTerminal": true,
  // Npm：验证依赖关系
