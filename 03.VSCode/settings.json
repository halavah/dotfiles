  "[javascript|react]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Prettier：配置文件格式（typescript|react）
  "[typescript|react]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

