{
  // 窗口：缩放级别
  "window.zoomLevel": 0,
  // 窗口：侧边栏样式
  "window.titleBarStyle": "custom",
  // 窗口：默认文件夹
  "window.restoreWindows": "folders",
  // 窗口：新打开文件
  "window.openFilesInNewWindow": "on",
  // 窗口：隐藏主菜单
  "window.menubarvisibility": "toggle",
  // 编辑：缩进空格数量
  "editor.tabSize": 2,
  // 编辑：设置文字大小
  "editor.fontSize": 14,
  // 编辑：添加自动换行
  "editor.wordWrap": "off",
  // 编辑：换行的字符数
  "editor.wordWrapColumn": 120,
  // 编辑：换行的提示线
  "editor.rulers": [120],
  // 编辑：设置代码行号
  "editor.lineNumbers": "off",
  // 编辑：格式编辑内容
  "editor.formatOnType": true,
  // 编辑：格式编辑内容
  "editor.formatOnSave": true,
  // 编辑：缩进插入空格
  "editor.insertSpaces": true,
  // 编辑：格式编辑内容
  "editor.formatOnPaste": true,
  // 编辑：填入最佳推荐
  "editor.tabCompletion": "on",
  // 编辑：滚轮缩放代码
  "editor.mouseWheelZoom": true,
  // 编辑：显示颜色预览
  "editor.colorDecorators": true,
  // 编辑：迷你地图显示
  "editor.minimap.enabled": false,
  // 编辑：缩进自动检测
  "editor.detectIndentation": false,
  // 编辑：显示空白字符
  "editor.renderWhitespace": "none",
  // 编辑：选中首个推荐
  "editor.suggestSelection": "first",
  // 编辑：缩排的辅助线
  "editor.renderIndentGuides": false,
  // 编辑：提示代码建议
  "editor.snippetSuggestions": "top",
  // 编辑：插入多个光标
  "editor.multiCursorModifier": "alt",
  // 编辑：渲染高亮显示
  "editor.renderLineHighlight": "all",
  // 编辑：编辑显示预览
  "editor.rename.enablePreview": false,
  // 编辑：显示控制字符
  "editor.renderControlCharacters": true,
  // 编辑：基于缩进折叠
  "editor.foldingStrategy": "auto",
  // 编辑：style无提示
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  // 编辑：设置文字样式
  "editor.fontFamily": "JetBrains Mono, 'Courier New', monospace",
  // 编辑：选中间隔符号
  "editor.wordSeparators": "`~!@$%^&*()=+[{]}\\|;:'\",.<>/?“”（）【】《》 ：、；，。",
  // 编辑：代码立即修复
  "editor.codeActionsOnSave": {
    // "source.fixAll.eslint": true,
    // "source.fixAll.tslint": true,
    "source.fixAll.stylelint": true,
    "source.organizeImports": false
  },
  // 编辑：快速建议内容
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },

  // 文件：设置文件编码
  "files.encoding": "utf8",
  // 文件：自动保存文件
  "files.autoSaveDelay": 10,
  // 文件：自动保存文件
  "files.autoSave": "afterDelay",
  // 文件：自动匹配中文
  "files.autoGuessEncoding": true,
  // 文件：剪裁多余空格
  "files.trimTrailingWhitespace": true,
  // 文件：语言文件关联
  "files.associations": {
    "*.vue": "vue",
    "*.wpy": "vue",
    "*.wxss": "css",
    "*.wxml": "html",
    "*.cjson": "jsonc",
    "*.js": "javascript",
    "*.wxs": "javascript"
  },
  // 文件：显示排除文件
  "files.exclude": {
    "**/.hg": true,
    "**/CVS": true,
    "**/.git": true,
    "**/.svn": true,
    "**/*.iml": true,
    "**/.idea": true,
    "**/target": true,
    ".gitignore": true,
    "**/.project": true,
    "**/.settings": true,
    "**/rebel.xml": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.factorypath": true
  },

  // 搜索：搜索排除文件
  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true,
    "**/*.code-search": true,
    "**/bower_components": true
  },

  // 终端：设置字体大小
  "terminal.integrated.fontSize": 14,
  // 终端：设置字体样式
  "terminal.integrated.fontFamily": "JetBrains Mono, 'Courier New', monospace",

  // 管理器：提示删除文件
  "explorer.confirmDelete": false,
  // 管理器：提示删除文件
  "explorer.confirmDragAndDrop": false,
  // 工作台：设置界面形式
  "workbench.settings.editor": "json",
  // 工作台：是否启用预览
  "workbench.editor.enablePreview": false,
  // 工作台：设置编辑主题
  "workbench.colorTheme": "Monokai Dimmed",
  // 工作台：设置图标主题
  "workbench.iconTheme": "vscode-great-icons",
  // 工作台：无标题的提示
  "workbench.editor.untitled.hint": "hidden",
  // 工作台：默认新建文本
  "workbench.startupEditor": "newUntitledFile",
  // 工作台：打开对照设置
  "workbench.settings.openDefaultSettings": false,
  // 工作台：自动键盘导航
  "workbench.list.automaticKeyboardNavigation": false,
  // 工作台：自定义主题色
  "workbench.colorCustomizations": {
    "terminal.background": "#212121",
    "terminal.foreground": "#EEFFFF",
    "terminalCursor.background": "#EEFFFF",
    "terminalCursor.foreground": "#EEFFFF",
    "terminal.ansiBlack": "#212121",
    "terminal.ansiBlue": "#82AAFF",
    "terminal.ansiBrightBlack": "#4A4A4A",
    "terminal.ansiBrightBlue": "#82AAFF",
    "terminal.ansiBrightCyan": "#89DDFF",
    "terminal.ansiBrightGreen": "#C3E88D",
    "terminal.ansiBrightMagenta": "#C792EA",
    "terminal.ansiBrightRed": "#F07178",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#FFCB6B",
    "terminal.ansiCyan": "#89DDFF",
    "terminal.ansiGreen": "#C3E88D",
    "terminal.ansiMagenta": "#C792EA",
    "terminal.ansiRed": "#F07178",
    "terminal.ansiWhite": "#EEFFFF",
    "terminal.ansiYellow": "#FFCB6B"
  },

  // 其他设置：打开调试界面
  "debug.openDebug": "neverOpen",
  // 其他设置：即将完成动作
  "todohighlight.isEnable": true,
  // 其他设置：忽略报错信息
  "vetur.ignoreProjectWarning": true,
  // 其他设置：忽略报错信息
  "json.schemaDownload.enable": false,
  // 其他设置：启动任务错误
  "debug.onTaskErrors": "debugAnyway",
  // 其他设置：任何地方调试
  "debug.allowBreakpointsEverywhere": true,
  // 其他设置：忽略空白空间
