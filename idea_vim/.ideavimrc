" =============================================
" 02.plugin 2
" =============================================

" ----------------------
" a.easymotion
" ----------------------
set easymotion
let g:EasyMotion_smartcase = 1
" let g:EasyMotion_use_upper = 0
map f  <Plug>(easymotion-bd-jk)
map F  <Plug>(easymotion-lineanywhere)
map gh <Plug>(easymotion-bd-w)
map gl <Plug>(easymotion-bd-e)
map <Leader><leader>h <Plug>(easymotion-linebackward)
map <Leader><Leader>j <Plug>(easymotion-j)
map <Leader><Leader>k <Plug>(easymotion-k)
map <Leader><leader>l <Plug>(easymotion-lineforward)
map <Leader><leader>s <Plug>(easymotion-s)
map <Leader><leader>S <Plug>(easymotion-s2)

" ----------------------
" b.multiple-cursors
" ----------------------
set multiple-cursors
nmap <A-B>   <Plug>SkipOccurrence
xmap <A-B>   <Plug>SkipOccurrence
nmap <A-N>   <Plug>NextWholeOccurrence
xmap <A-N>   <Plug>NextWholeOccurrence
nmap <S-A-N> <Plug>AllWholeOccurrences
xmap <S-A-N> <Plug>AllWholeOccurrences
nmap <A-M>   <Plug>RemoveOccurrence
xmap <A-M>   <Plug>RemoveOccurrence

