" =========================================== 06.Mode-KeyMap ===========================================

" =============================================
" :actionlist 查询命令
"
" normal: ESC
" visual: v
" insert: a A i I o O
" =============================================

" =============================================
" 01.normal mode
" =============================================

nnoremap k gk
nnoremap gk k
nnoremap j gj
nnoremap gj j

nnoremap H ^
nnoremap L $
nnoremap K 3k
nnoremap J 3j

nnoremap ; :
nnoremap : ;

nnoremap # *
nnoremap * #

nnoremap ' `
nnoremap ` '

nnoremap <A-E> j
nnoremap <A-Y> k
nnoremap <A-D> <C-d>
nnoremap <A-U> <C-u>

" =============================================
" 02.visual modes
" =============================================

vnoremap k gk
vnoremap gk k
vnoremap j gj
vnoremap gj j

vnoremap H ^
vnoremap L $
vnoremap K 3k
vnoremap J 3j

vnoremap ; <ESC>:
vnoremap : <ESC>;

vnoremap # <ESC>*
vnoremap * <ESC>#

vnoremap <A-E> j
vnoremap <A-Y> k
vnoremap <A-D> <C-d>
vnoremap <A-U> <C-u>

" =============================================
" 03.insert mode
" =============================================

inoremap <A-H> <Left>
inoremap <A-J> <Down>
inoremap <A-K> <Up>
inoremap <A-L> <Right>
inoremap <A-A> <Esc>^i
inoremap <A-E> <Esc>$a
inoremap <A-D> <Delete>

inoremap ( ()<Esc>i
inoremap [ []<Esc>i
inoremap { {}<Esc>i
inoremap " ""<Esc>i


" =========================================== 07.Multi-Window ==========================================

" =============================================
" :actionlist 查询命令
"
" file    : q  n  m
" window  : wv ws ww wd wh wj wk wl
" bookmark: nn mm m, m. m[ m] ms `s
" =============================================

" =============================================
" 01.file operation
" =============================================

noremap <C-Z>      <ESC>
noremap <S-Z><S-Z> <ESC>
noremap <leader><ESC> <ESC>:nohlsearch<CR>

noremap <leader>q  <ESC>:wq!<CR>
noremap <leader>n  <ESC>:e C:/Users/mysla/.ideavimrc<CR>
noremap <leader>m  <ESC>:source C:/Users/mysla/.ideavimrc<CR>

" =============================================
" 02.window operation
" =============================================

noremap <Leader>wd <ESC><C-W>c
noremap <leader>ww <ESC><C-W>w

noremap <leader>wv <ESC><C-W>v
noremap <leader>ws <ESC><C-W>s

noremap <leader>wh <ESC><C-W>h
noremap <leader>wj <ESC><C-W>j
noremap <leader>wk <ESC><C-W>k
noremap <leader>wl <ESC><C-W>l

noremap <Left>     <ESC><C-W>h
noremap <Down>     <ESC><C-W>j
noremap <Up>       <ESC><C-W>k
noremap <Right>    <ESC><C-W>l

